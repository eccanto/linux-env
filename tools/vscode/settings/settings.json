{
    // "editor.fontSize": 12,
    // "terminal.integrated.fontSize": 12,
    // "window.zoomLevel": -1,
    "editor.fontSize": 12,
    "terminal.integrated.fontSize": 12,
    "editor.wordWrap": "off",
    "[markdown]": {
        "editor.wordWrap": "off",
    },
    "workbench.editor.showTabs": "multiple",
    "editor.minimap.enabled": false,
    "editor.scrollbar.vertical": "auto",
    "editor.overviewRulerBorder": false,
    "editor.hideCursorInOverviewRuler": true,
    "python.analysis.autoImportCompletions": true,
    "editor.guides.indentation": false,
    "editor.glyphMargin": false,
    "python.terminal.activateEnvironment": false,
    "workbench.sideBar.location": "left",
    "indenticator.width": 1,
    "indenticator.color.dark": "rgba(255, 255, 255, 0.3)",
    "svgPreview.autoOpen": false,
    "clock.dateFormat": "mm/dd/yyyy - HH:MM",
    "editor.renderWhitespace": "all",
    "indentRainbow.colors": [
        "rgba(100, 100, 100, 0.075)",
        "rgba(79, 200, 200, 0.075)"
    ],
    "workbench.editor.tabSizing": "fit",
    "autoDocstring.docstringFormat": "sphinx",
    "workbench.colorCustomizations": {
        "editorWhitespace.foreground": "#2e2e2e",
        "editor.background": "#000000",
        "menu.background": "#000000",
        "panel.background": "#000000",
        "minimap.background": "#000000",
        "breadcrumb.background": "#000000",
        "sideBar.background": "#000000",
        "terminal.background": "#000000",
        "statusBar.background": "#000000",
        "editorGroupHeader.tabsBackground": "#000000",
        "tab.activeBackground": "#000000",
        "tab.inactiveBackground": "#000000"
    },
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/__pycache__": true,
        ".vscode": true,
        ".venv": true,
        ".mypy_cache": true
    },
    "editor.rulers": [
        120
    ],
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "*.declaration": {
                "foreground": "#B6B6B6"
            },
            "class.declaration": {
                "foreground": "#C6C600",
                "bold": true
            },
            "parameter.declaration": {
                "foreground": "#ffa657"
            },
            "function.declaration": {
                "foreground": "#d2a8ff"
            },
            "method.declaration": {
                "foreground": "#d2a8ff"
            },
            "variable.builtin": {
                "foreground": "#66B2FF"
            }
        },
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "string.quoted.single",
                    "string.quoted.double",
                    "string.quoted.triple",
                    "string.quoted.multi.python",
                    "string.quoted.docstring.multi.python",
                ],
                "settings": {
                    "foreground": "#D0C139"
                }
            },
            {
                "scope": [
                    "source.shell",
                ],
                "settings": {
                    "foreground": "#B6B6B6"
                }
            },
            {
                "scope": [
                    "variable.other.bracket.shell",
                ],
                "settings": {
                    "foreground": "#66B2FF"
                }
            }
        ]
    },
    "editor.fontFamily": "Hack Nerd Font",
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.scrollback": 100000,
    "redhat.telemetry.enabled": false,
    "terminal.integrated.tabs.enabled": false,
    "window.menuBarVisibility": "hidden",
    "hediet.vscode-drawio.theme": "Kennedy",
    "cSpell.userWords": [
        "arange",
        "assertpy",
        "boxplot",
        "Ccanto",
        "cmap",
        "coloredlogs",
        "drawio",
        "facecolor",
        "fancybox",
        "figsize",
        "glassit",
        "hediet",
        "hspace",
        "indenticator",
        "ipdb",
        "isort",
        "levelname",
        "matplotlib",
        "mccabe",
        "msecs",
        "mypy",
        "ncols",
        "nrows",
        "playsound",
        "psql",
        "psutil",
        "pyautogui",
        "pycache",
        "pycodestyle",
        "pydocstyle",
        "pyenv",
        "pyflakes",
        "pylint",
        "pyplot",
        "pyroma",
        "pytest",
        "rgba",
        "savefig",
        "scrollback",
        "shellcheck",
        "titlebar",
        "venv",
        "veryhigh",
        "vmax",
        "vmin",
        "vsicons",
        "workdir",
        "wspace",
        "xaxis",
        "xlabel",
        "xlim",
        "xlsxwriter",
        "xticklabels",
        "xticks",
        "yaxis",
        "ylabel",
        "ylim",
        "yticks"
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "off",
    "[python]": {
        "editor.formatOnType": true
    },
    "vsicons.dontShowNewVersionMessage": true,
    "telemetry.telemetryLevel": "off",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "GitHub Dark Default",
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.folders.color": "#66B2FF",
    "terminal.integrated.env.linux": {},
    "glassit.alpha": 215,
    "workbench.activityBar.location": "hidden",
    "update.showReleaseNotes": false,
    "console-ninja.featureSet": "Community"
}
